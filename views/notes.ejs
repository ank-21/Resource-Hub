<div class="demo-wrapper">
  
  <!-- <h1>Horizontal Portfolio Layout With CSS3 Animations and jQuery</h1> -->
  <%= console.log("notes in ejs of notes",notes) %>
  <%= console.log(notes.length) %>


    <ul class="portfolio-items">

        

        <% notes.forEach((note) => { %>
            <li class="item">
                <figure>
                    <div class="view">
                    <img src="https://s.cdpn.io/9674/1.jpg" />
                    </div>
                    <figcaption>
                    <p><span><a href="<%= note.notesLoc %>" target="_blank"><%= note.subject %></a></span></p>
                    <!-- <p><span>Professor - <%= note.profName %></span></p> -->
                    <p><span>By <%= note.userName %></span></p>
                    </figcaption>
                </figure>
                <div class="date">4.3</div>
                </li>
        <% }); %>
    </ul>
